{
  "name": "@sansa-dev/s-nest",
  "version": "0.0.1",
  "description": "S Backend for Sansa AI Router - runs on port 3000, uses s-db PostgreSQL database",
  "author": "Sansa AI Team",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "type-check": "tsc --noEmit",
    "db:migrate": "node run-migration.js",
    "db:seed": "echo 'Database seeding not yet configured'",
    "docker:dev": "docker-compose --env-file .env.dev -f docker-compose.dev.yml up",
    "docker:dev:build": "docker-compose --env-file .env.dev -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker-compose --env-file .env.dev -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose --env-file .env.dev -f docker-compose.dev.yml logs -f",
    "deploy": "gcloud builds submit --config cloudbuild.yaml",
    "deploy:prod": "npm run build && npm run deploy"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.33.1",
    "@aws-sdk/client-s3": "^3.879.0",
    "@aws-sdk/s3-request-presigner": "^3.879.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bull": "^11.0.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.6",
    "@sendgrid/mail": "^8.1.5",
    "@types/bull": "^4.10.4",
    "@types/dockerode": "^3.3.43",
    "@types/uuid": "^10.0.0",
    "@sansa-dev/s-shared": "workspace:*",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "cache-manager": "^6.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dockerode": "^4.0.7",
    "express": "^5.1.0",
    "install": "^0.13.0",
    "ioredis": "^5.6.1",
    "minio": "^8.0.5",
    "openai": "^4.77.0",
    "pg": "^8.16.2",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "tar-stream": "^3.1.7",
    "typeorm": "^0.3.24",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.2",
    "@types/node": "catalog:",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "jest": "^29.5.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "catalog:"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
